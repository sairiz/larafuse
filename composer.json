{
    "name": "sairiz/larafuse",
    "description": "Infusionsoft Database Syncronizer for Laravel 4",
    "keywords": ["laravel", "infusionsoft"],
    "authors": [
        {
            "name": "Saiffil Fariz",
            "email": "saiffil@mtouch.my"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "sairiz/infusionsoft": "1.*",
        "sairiz/mandrill": "1.*",
        "liebig/cron": "0.9.*",
        "laravelbook/ardent": "2.4.*"
    },
    "autoload": {
        "classmap": [
            "src/seeds",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Sairiz\\Larafuse\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    }
}
